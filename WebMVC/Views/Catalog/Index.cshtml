@{
}
@model WebMVC.Models.Catalog

@foreach (var item in Model.Data)
{
    <tr>
        <td>
            @item.Name
        </td>
        <td>
            @item.Description
        </td>
        <td>
            @item.Price
        </td>
        <td>
            <img src="@item.PictureUrl" />
        </td>
    </tr>
}
    